# Supabase configuration for Zero to AI Hub
# Copy this file to .env and fill in the values from your Supabase project.

# Public anon key from Settings > API > Project API keys
# Required to enable the hosted Supabase project integration
SUPABASE_KEY=

# Service role key from Settings > API > Project API keys
# Required by the migration script to push the database schema
SUPABASE_SERVICE_ROLE_KEY=

# Optional override for the hosted Supabase project URL
SUPABASE_URL=

# Optional overrides for local development builds that still rely on Vite env vars
VITE_SUPABASE_URL=
VITE_SUPABASE_PUBLISHABLE_KEY=
